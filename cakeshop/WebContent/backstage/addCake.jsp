	        <form action="${pageContext.request.contextPath}/admin/findCake" method="post">
	        	<ul>蛋糕：<input type="text" name="cakeName" /></ul>
	        	<ul>描述：<input type="text" name="description" /></ul>
	        	<ul>图片1：<input type="file" name="p1"/></ul>
	        	<ul>图片2：<input type="file" name="p2"/></ul>
	        	<ul>图片3：<input type="file" name="p3"/></ul>
	        	<ul>种类：
	        		<select name="typeName">
		        		<c:forEach items="${types }" var="type">
		        			<option  value="${type.typeName }">${type.typeName }</option>
		        		</c:forEach>
	        		</select> 
	        	</ul>
	        	<ul>口味：
	        		<select name="flavourName">
		        		<c:forEach items="${flavours }" var="flavour">
		        			<option  value="${flavour.flavourName }">${flavour.flavourName }</option>
		        		</c:forEach>
	        		</select> 
	        	</ul>
	        	<ul>颜色：
	        		<select name="colorName">
		        		<c:forEach items="${colors }" var="color">
		        			<option  value="${color.colorName }">${color.colorName }</option>
		        		</c:forEach>
	        		</select> 
	        	</ul>
	        	<ul><input type="reset" value="重置"/>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<input type="submit" value="修改无误，提交"/></ul>
